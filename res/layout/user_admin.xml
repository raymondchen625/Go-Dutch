<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:orientation="vertical">
  

<LinearLayout android:id="@+id/LinearLayout01" android:layout_width="wrap_content" android:layout_height="wrap_content">
<Button android:text="@string/newUser" android:id="@+id/addUserButton01" android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
<Button android:text="@string/importFromContacts" android:id="@+id/importFromContactsButton" android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
</LinearLayout>

<ListView android:id="@+id/currentUsersListView" android:layout_width="wrap_content" android:layout_height="wrap_content"></ListView>


</LinearLayout>
